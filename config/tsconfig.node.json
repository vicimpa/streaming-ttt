{
  "compilerOptions": {
    "tsBuildInfoFile": null,
    "target": "esnext",
    "lib": [
      "esnext"
    ],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true,
    "moduleDetection": "force",
    "noEmit": true,
    "strict": true,
  },
  "include": [
    "../vite.config.ts",
    "../server"
  ]
}