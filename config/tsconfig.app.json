{
  "compilerOptions": {
    "tsBuildInfoFile": null,
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": [
      "ESNext",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "verbatimModuleSyntax": true,
    "moduleDetection": "force",
    "noEmit": true,
    "jsx": "react-jsx",
    "strict": true,
    "types": [
      "vite/client",
      "vite-plugin-svgr/client",
      "vite-plugin-glsl/ext"
    ]
  },
  "include": [
    "../src"
  ]
}